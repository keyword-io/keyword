{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "generate": {
      "outputs": ["lib/**"],
      "dependsOn": [
        "^generate"
      ]
    },
    "build": {
      "outputs": ["lib/**"],
      "dependsOn": [
        "^build",
        "^generate"
      ]
    },
    "type-check": {},
    "prepublish": {
      "outputs": ["lib/**"],
      "dependsOn": [
        "^prepublish",
        "^generate"
      ]
    }
  }
}
