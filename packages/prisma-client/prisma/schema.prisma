generator client {
  provider      = "prisma-client-js"
  output        = "./client"
  binaryTargets = ["native", "darwin"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(uuid())
  email       String   @unique
  name        String?
  create_time DateTime @default(now())
  update_time DateTime
}

model Feed {
  id          String   @id @default(uuid())
  title       String
  content     String
  create_time DateTime @default(now())
  update_time DateTime
}
